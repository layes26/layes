#include<stdio.h>
int main(){
   int N;
   while((scanf("%d",&N) && N!=0)){
         int x,y,p1=0,p2=0;
         while(N--){
            scanf("%d %d",&x,&y);
            if(x>y)
            {
                p1++;
            }
            else if(x<y)
            {
                p2++;
            }
            else if(x==y)
                p1==p2==0;
         }
         printf("%d %d\n",p1,p2);

   }
    return 0;
}
